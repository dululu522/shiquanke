/* CSS Document */


body{
	font-family: 宋体;
	font-size:9pt;
	color:#5C5C5C;
	text-decoration: none;
	line-height: 12pt;
	SCROLLBAR-ARROW-COLOR: #000000; 
	SCROLLBAR-TRACK-COLOR: #ffffff; 
	SCROLLBAR-HIGHLIGHT-COLOR: #ffffff; 
	SCROLLBAR-3DLIGHT-COLOR: #ffffff; 
	SCROLLBAR-SHADOW-COLOR: #cccccc; 
	SCROLLBAR-DARKSHADOW-COLOR: #ffffff;
	margin-top: 0px;
	margin-right: 0px;
	margin-bottom: 0px;
	margin-left: 0px;
	background-color: #FFFFFF;
	
	
}
TH{font-family: 宋体; FONT-SIZE: 9pt; text-decoration: none; line-height: 12pt; color:#5C5C5C; }
TD{font-family: 宋体; FONT-SIZE: 9pt; text-decoration: none; line-height: 12pt; color:#5C5C5C; }


.normal_input {
	font-family: 宋体;
	BORDER-TOP-WIDTH: 1px;
	BORDER-LEFT-WIDTH: 1px; 
	FONT-SIZE: 9pt; 
	BORDER-LEFT-COLOR: #5C5C5C; 
	BORDER-BOTTOM-WIDTH: 1px; 
	BORDER-BOTTOM-COLOR: #5C5C5C; 
	COLOR: #828282; 
	BORDER-TOP-COLOR: #5C5C5C; 
	BACKGROUND-COLOR: #ffffff;
	BORDER-RIGHT-WIDTH: 1px; 
	BORDER-RIGHT-COLOR: #5C5C5C
}
.normal_input2 {
	font-family: 宋体;
	BORDER-RIGHT: gold 1px solid;
	BORDER-TOP: gold 1px solid;
	FONT-SIZE: 9pt;
	BORDER-LEFT: gold 1px solid;
	BORDER-BOTTOM: gold 1px solid;
	CURSOR: hand;
	BACKGROUND-COLOR: YELLOW;
	height: 20;
}

.normal{
	font-family: 宋体;
	font-size:9pt;
	color:#5C5C5C;
	text-decoration: underline;
}
.normal:hover{
text-decoration: none;
color:#989797;
}
.right{
	text-align:right;}
